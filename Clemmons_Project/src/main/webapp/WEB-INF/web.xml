<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>



<servlet>
  <servlet-name>tickets</servlet-name>
  <servlet-class>controller.TicketServlet</servlet-class>
</servlet>



  <servlet>
    <servlet-name>employee</servlet-name>
    <servlet-class>controller.EmployeeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>employee</servlet-name>
    <!-- we include the * meaning anything can come after this -->
    <url-pattern>/employee</url-pattern>
  </servlet-mapping>



  <servlet-mapping>
    <servlet-name>tickets</servlet-name>
    <!-- we include the * meaning anything can come after this -->
    <url-pattern>/tickets/*</url-pattern>
  </servlet-mapping>







</web-app>
